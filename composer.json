{
    "name": "beapp/mailer",
    "description": "Mailer library for Symfony project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Damien Villeneuve",
            "email": "d.villeneuve@beapp.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/log": "1.0.2",
        "ext-json": "*",
        "mailgun/mailgun-php": "^2.3",
        "mandrill/mandrill": "^1.0",
        "pda/pheanstalk": "^3.2",
        "symfony/translation": "4.*",
        "symfony/templating": "4.*",
        "symfony/routing": "4.*"
    },
    "replace": {
        "beapp/mailer-core": "self.version",
        "beapp/mailer-client-mailgun": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Beapp\\Email\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
